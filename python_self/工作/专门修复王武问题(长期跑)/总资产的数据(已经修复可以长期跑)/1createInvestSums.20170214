CREATE TABLE invest.t_investment_sums (
  `investor_uid` bigint(20) NOT NULL DEFAULT '0' COMMENT '真实投资人UID',
  `loan_invest_sum` bigint(20) NOT NULL DEFAULT '0' COMMENT '累计投资金额',
  `loan_invest_payoff` bigint(20) NOT NULL DEFAULT '0' COMMENT '已经赚取收益',
  `loan_principal` bigint(20) NOT NULL DEFAULT '0' COMMENT '待收本金',
  `loan_interest` bigint(20) NOT NULL DEFAULT '0' COMMENT '待收利息',
  `loan_add_interest` bigint(20) NOT NULL DEFAULT '0' COMMENT '待收加息',
  `loan_invests` bigint(20) NOT NULL DEFAULT '0' COMMENT '累计成功投资的笔数',
  `plan_invest_sum` bigint(20) NOT NULL DEFAULT '0' COMMENT '累计投资金额',
  `plan_invest_payoff` bigint(20) NOT NULL DEFAULT '0' COMMENT '已经赚取收益',
  `plan_principal` bigint(20) NOT NULL DEFAULT '0' COMMENT '待收本金',
  `plan_interest` bigint(20) NOT NULL DEFAULT '0' COMMENT '待收利息',
  `plan_add_interest` bigint(20) NOT NULL DEFAULT '0' COMMENT '待收加息',
  `plan_invests` bigint(20) NOT NULL DEFAULT '0' COMMENT '当前持有债权数',
  `debt_invest_sum` bigint(20) NOT NULL DEFAULT '0' COMMENT '累计已转让初始金额',
  `debt_actual_amount` bigint(20) NOT NULL DEFAULT '0' COMMENT '实际成交金额',
  `debt_invests` bigint(20) NOT NULL DEFAULT '0' COMMENT '累计成交次数',
  `loan_yesterday_payoff` bigint(20) NOT NULL DEFAULT '0' COMMENT '昨日定期理财收益',
  `loan_today_payoff` bigint(20) NOT NULL DEFAULT '0' COMMENT '今天定期理财收益',
  `plan_yesterday_payoff` bigint(20) NOT NULL DEFAULT '0' COMMENT '昨日理财计划收益',
  `plan_today_payoff` bigint(20) NOT NULL DEFAULT '0' COMMENT '今日理财计划收益',
  `update_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '更新时间',
  `create_time` datetime NOT NULL DEFAULT '0000-00-00 00:00:00' COMMENT '创建时间',
  `xrdt_invest_sum` bigint(20) NOT NULL DEFAULT '0' COMMENT '累计投资金额',
  `xrdt_invest_payoff` bigint(20) NOT NULL DEFAULT '0' COMMENT '已经赚取收益',
  `xrdt_principal` bigint(20) NOT NULL DEFAULT '0' COMMENT '待收本金',
  `xrdt_interest` bigint(20) NOT NULL DEFAULT '0' COMMENT '待收利息',
  `xrdt_add_interest` bigint(20) NOT NULL DEFAULT '0' COMMENT '待收加息',
  `xrdt_invests` bigint(20) NOT NULL DEFAULT '0' COMMENT '累计成功投资的笔数',
  PRIMARY KEY (`investor_uid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8